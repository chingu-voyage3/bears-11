          "react-hmre"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "browsers": ["last 2 Chrome versions"]
              },
              "es2015": {
                "modules": false
              }
            }
          ]
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "env"
        ]
      }
    }
